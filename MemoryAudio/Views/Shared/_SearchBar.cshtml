
@using (Html.BeginForm("Results", "Search", FormMethod.Get, new { @class = "customform s-12 l-8 pull-right" }))
{
    <div class="s-9">@Html.TextBox("SearchText", ViewData["SearchText"] ?? "", new { @type = "text", @placeholder = "Nhập từ khóa tra cứu", @title = "Tra cứu" })</div>
    <div class="s-3"><button type="submit">Tra cứu</button></div>
}
